.mainFrame {
	position: absolute;
	width: 100%;
	height: 100%;
	font-family: 'Catamaran', 'Segoe UI', Tahoma, Calibri, Geneva, Verdana, sans-serif;
}
.column {
	%border-width: thin;
	font-size: large;
	line-height: 150%;
	text-align: justify;
	text-indent: 5pt;
	position: relative;
	width: 70%;
	min-height: 100%;
	max-width: 640px;
	min-width: 480px;
	margin-right: auto;
	margin-left: auto;
	margin-top: 1em;
	%border-right-style: dotted;
	%border-left-style: dotted;
	%padding-left: 5pt;
	%padding-right: 5pt;
	padding-top: 5pt;
	padding-bottom: 5pt;
	background-color: #FFFFFF;
	color: #464646;
	%box-shadow: 3px 3px 3px 2px #999999;
}
.navwrap{
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index: 1;
}

.row {
	%%display:table-row;
	%font-family: 'Segoe UI', Tahoma, Calibri,Geneva, Verdana, serif;
	%font-family: 'Source Sans Pro', sans-serif;
	font-size: 120%;
	font-weight: 600;
	%font-variant: small-caps;
	%%text-align:	center;
	vertical-align:middle;
	%%position: fixed;
	%%top: 0;
	%%left: 0;
	%%right: 0;
	%%z-index: 1;
	height: 1.5em;
	%background-color: #FFFFFF;%#674C54;
	%background-color: #646464;
	background-color: rgba(24,24,24,0.5);
	width: 480px;
	%max-width: 640px;
	%min-width: 480px;
	margin-right: auto;
	margin-left: auto;
%	color: #FFFFFF;
	padding-left: 5pt;
	padding-right: 5pt;
	%border-color: #646464;
	%border-style:solid;
	%border-width:thin;
	border-radius: 8px;
	box-shadow: 5px 5px 5px #888888;
}
#homemenu.menu{
    float: left;
	vertical-align:middle;	
	width: auto;
    %min-width: 16pt;
	height: inherit;
	padding: 0pt 1em 0pt 1em;
    background-image: url("home.png");
    background-size: 14pt;
    background-repeat: no-repeat;
    background-position: center;    
}
#homemenu.menu:hover{
    background-image: url("home-active.png");
}
#homemenu.menu:link{
        
}

.menu{
    float: left;
	vertical-align:middle;
	text-align: left;	
	width: auto;
	height: 100%;
	%margin-right: auto;
	%margin-right: auto;
	%padding-right: 1em;
	%padding-left: 4pt;
	padding: 0pt 1em 0pt 1em;
	%border-style:solid;
	%border-radius: 8px 8px 0px 0px;
	/*min-width: 6em;*/
    
    color: #FFFFFF;
    text-decoration: none;
}

.menu:hover{
	%background-color: rgb(64,176,225);
	color: #FD970D;
	%background-color: #F3F0E9;
	%background-color: #F8F5F1;
	cursor: pointer;
}

.vsep{
	float: left;
	vertical-align: middle;
	height: 100%;
	width: 3pt;
}

.item{
	box-shadow: 4px 3px 3px #808080;
	border: thin solid #808080;
	background-color: #F8F5F1;
	border-radius: 8px;
	margin-right: auto;
	margin-left: auto;
	%width: inherit;
	margin-bottom: 5pt;
	padding-left: 5pt;
	padding-right: 5pt;
	padding-bottom: 5pt;
	%padding-top: 1.5em;    
}

.semibold{
	font-weight: 600;
}
.disabled{
	position: relative;
	width: auto;
	height: 1.5em;
	float: left;
	padding-right: 1em;
	padding-left: 1em;
	color: #999999;
}
.equation{
	color: inherit;%#000000;
}
.equation:hover{
	color: inherit;%#000000;
}
.hlink{
	border-radius: 3px;
	background-color: #0099FF;
	color: white;
	% font-weight: 600;
	text-decoration: none;
	margin-left: 1pt;
	margin-right: 1pt;
	padding-left: 1ex;
	padding-right: 1ex;
	margin-left: 1pt;
	font-size: 80%;
}
.hlink:hover{
	background-color: #FD970D;
	color: white;
}
.hclink{
	border-radius: 3px;
	background-color: #46DEBD;
	color: white;
	% font-weight: 600;
	text-decoration: none;
	margin-left: 1pt;
	margin-right: 1pt;
	padding-left: 1ex;
	padding-right: 1ex;
	margin-left: 1pt;
	font-size: 80%;
}
.hclink:hover{
	background-color: #FD970D;
	color: white;
}

.anchor{
    display: block;
    height: 1.8em;
    margin-top: -1.8em;
    visibility: hidden;
}

.eqanchor{
    display: block;
    padding-top: 1.5em;
    margin-top: -1.5em;
}

a{
	color: rgb(0,112,192);
	%font-weight: bold;
}
a:hover{
	color: rgb(64,176,225);
	%font-weight: bold;
}

h4{
	color: #D17702;
	font-size: 125%;
	text-indent: 0;
	margin-top: 1ex;
	margin-bottom: 0.5ex;
	clear: both;
}
h2{
	clear: both;
	margin-top: 1ex;
	margin-bottom: 0.5ex;
	text-indent: 0;
	%color: #D17702;%rgb(64,176,225);
}
h5.highlight{
	%background: #3366FF;
	font-size: 90%;
	color: #0070C0;%white;
	text-indent: 0;
	margin-bottom: 2pt;
}

p{
	margin-top: 0pt;
	margin-bottom: 5pt;
}
code{
	font-family: "Courier New", Courier, monospace;
	font-size: 90%;
}
span.warn{
	color: #990000;
	text-decoration: underline;
}

.paper {
 	margin: 0;
	padding: 0;
	list-style-type: none; 	
}

ul.paper li{
	float: left;
}
ol.paper.begin{
	counter-reset: paper-cnt;
}
.paper li {
	text-indent: -2em;
	padding-left: 2em;
	margin-left: 1ex;
	margin-right: 1ex;
	margin-top: 0.5ex;
	font-size: 95%;
	margin-bottom: 0.5ex; 
}
ol.paper li::before{
	content: counter(paper-cnt);
	background-color: #FFFFFF;
	font-size: 90%;
	font-weight:bold;
	border-radius: 3px;
	padding: 1px 5px;
	margin: 1ex;
	counter-increment: paper-cnt;
	
}
ul.paper li::before{
	content: " ";
	background-color: #FFFFFF;
	font-size: 90%;
	font-weight:bold;
	border-radius: 3px;
	padding: 1px 8px;
	margin: 1ex;
	counter-increment: paper-cnt;
}
.paper li.jrn::before {
	color:	#0099FF;
	border-style: solid;
}

.paper li.cnf::before {
	color: #46DEBD;
	border-style: solid;
}

.paper li.sub::before{
	color: #646464;
	border-style: double;
}

.paper li.rpt::before{
	color: #646464;
	border-style: solid;
}

ul.noindent {
    list-style-type: square;
    margin-left: 20px;
    padding-left: 0px;
}